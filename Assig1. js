let bool=true;
console.log(bool,typeof bool);  //check boolean value abd its type

let num=458.98;
console.log(num,typeof num);  //chech num value and its type

let symbol=('hello world');
console.log(symbol,typeof symbol);  //check string value and its type

let iphone={
        iphoneXR:{                // object with its attributes
             price:75000,
             rating:4.5
        }
    }
console.log(iphone,typeof iphone);   // printing attributes and its type

let arr=[1,2,3,4];   
console.log(arr,typeof arr) ;      //array and its type

